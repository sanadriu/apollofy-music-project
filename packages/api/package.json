      "url": "https://www.danilucaci.com/"
    },
    {
      "name": "Joan Carrion",
      "email": "joancarrion14@gmail.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/assembler-school/apollofy-music-project.git"
  },
  "type": "commonjs",
  "keywords": [
    "node.js"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-query-parser": "^1.3.2",
    "faker": "^5.5.3",
    "firebase-admin": "^9.5.0",
    "helmet": "^4.4.1",
    "loglevel": "^1.7.1",
    "mongodb-memory-server": "^6.9.6",
    "mongoose": "^6.1.9",
    "mongoose-lean-virtuals": "^0.9.0",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
